
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc/20980000.usb/usb1/1-1/1-1.3/1-1.3:1.0'
	option channel '1'
	option band '2g'
	option htmode 'HT20'
	option disabled '1'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'none'

